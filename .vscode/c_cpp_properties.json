{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "linux-gcc-arm",
            "compilerPath": "/opt/AVR/root/bin/avr-gcc",
            "cStandard": "gnu23",
            "cppStandard": "gnu++23",
            "includePath": [
                "${workspaceFolder}/eduardino",
                "${workspaceFolder}/**"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-pipe"
            ]
        }
    ],
    "version": 4
}