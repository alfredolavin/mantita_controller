{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "linux-gcc-arm",
            "cStandard": "c23",
            "cppStandard": "c++23",
            "includePath": [],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-pipe",
                "-Ieduardino",
                "-mmcu=attiny85",
                "-DF_CPU=8000000UL"
            ],
            "defines": [
                "__AVR_ATtiny85__=1",
                "F_CPU=8000000"
            ],
            "compilerPath": "/opt/AVR/root/bin/avr-gcc"
        }
    ],
    "version": 4
}